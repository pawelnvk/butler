{
  "name": "butler",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "final-form": "^4.9.1",
    "lodash.isundefined": "^3.0.1",
    "normalize.css": "^8.0.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-final-form": "^3.6.5",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "2.16.0",
    "styled-components": "^3.3.3",
    "stylelint": "^9.3.0"
  },
  "scripts": {
    "build": "react-scripts-ts build",
    "eject": "react-scripts-ts eject",
    "new:atom": "yarn new:component --scope atoms --name",
    "new:composer": "hygen composer new --name",
    "new:molecule": "yarn new:component --scope molecules --name",
    "new:organism": "yarn new:component --scope organisms --name",
    "new:template": "yarn new:component --scope templates --name",
    "new:view": "yarn new:component --scope views --name",
    "new:component": "hygen component new",
    "start": "react-scripts-ts start",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "css:lint": "stylelint ./src/**/*.styled.tsx",
    "svg-optimalize": "svgo src/assets/svg/*.svg",
    "test": "react-scripts-ts test --env=jsdom"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^4.0.0-alpha.3",
    "@storybook/addon-info": "^4.0.0-alpha.3",
    "@storybook/react": "^4.0.0-alpha.3",
    "@types/classnames": "^2.2.4",
    "@types/enzyme": "^3.1.11",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^23.1.1",
    "@types/node": "^10.3.5",
    "@types/react": "^16.4.1",
    "@types/react-dom": "^16.0.6",
    "@types/react-router-dom": "^4.2.7",
    "@types/storybook__addon-actions": "^3.0.3",
    "@types/storybook__addon-info": "^3.2.3",
    "@types/storybook__react": "^3.0.7",
    "awesome-typescript-loader": "^5.2.0",
    "babel-core": "^6.26.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "hygen": "^1.6.2",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.3.1",
    "svgo": "^1.0.5",
    "tslint-config-airbnb": "^5.9.2",
    "typescript": "^2.9.2",
    "typescript-styled-plugin": "^0.6.3",
    "url-loader": "^1.0.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.stories.{ts,tsx}",
      "!src/**/*.styled.{ts,tsx}",
      "!src/**/index.{ts,tsx}",
      "!src/registerServiceWorker.ts"
    ],
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1",
      "url-loader": "<rootDir>/node_modules/url-loader"
    }
  }
}
