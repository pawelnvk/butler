{
  "name": "butler",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts-ts": "2.16.0",
    "styled-components": "^3.3.3"
  },
  "scripts": {
    "build": "react-scripts-ts build",
    "eject": "react-scripts-ts eject",
    "new-atom": "yarn new-component --scope atoms",
    "new-molecule": "yarn new-component --scope molecules",
    "new-organism": "yarn new-component --scope organisms",
    "new-template": "yarn new-component --scope templates",
    "new-view": "yarn new-component --scope views",
    "new-component": "hygen component new",
    "start": "react-scripts-ts start",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "react-scripts-ts test --env=jsdom"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^4.0.0-alpha.3",
    "@storybook/addon-info": "^4.0.0-alpha.3",
    "@storybook/react": "^4.0.0-alpha.3",
    "@types/enzyme": "^3.1.11",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^23.1.1",
    "@types/node": "^10.3.5",
    "@types/react": "^16.4.1",
    "@types/react-dom": "^16.0.6",
    "@types/storybook__addon-actions": "^3.0.3",
    "@types/storybook__react": "^3.0.7",
    "awesome-typescript-loader": "^5.2.0",
    "babel-core": "^6.26.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "hygen": "^1.6.2",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "typescript": "^2.9.2",
    "typescript-styled-plugin": "^0.6.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.stories.{ts,tsx}",
      "!src/**/*.styled.{ts,tsx}",
      "!src/**/index.{ts,tsx}",
      "!src/registerServiceWorker.ts"
    ],
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1"
    }
  }
}
